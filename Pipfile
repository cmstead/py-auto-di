[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[scripts]
test = "python -m pytest"
dist = "python ./setup.py sdist --formats=gztar,zip"
test-publish = "twine upload --repository-url https://test.pypi.org/legacy/ dist/*"
publish = "twine upload dist/*"

[dev-packages]
pytest = "*"
pyautodi = {path = "."}
autopep8 = "*"
rope = "*"
py-auto-di = {path = "."}

[packages]

[requires]
python_version = "3.8"
